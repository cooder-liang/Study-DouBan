用其他方式登录 &amp; 找回密码</div>\r\n\t\t\t<div class=\"btns\">\r\n\t\t\t\t<router-link :to=\"{name:'RegisterView'}\">注册豆瓣账号</router-link>\r\n\t\t\t\t<a href=\"#\">下载豆瓣App</a>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {\r\n\t\tmapState\r\n\t} from 'vuex'\r\n\r\n\texport default {\r\n\t\tname: 'login-view',\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tloginState: '登录',\r\n\t\t\t\tisDisabled: false,\r\n\t\t\t\tisShow: 0,\r\n\t\t\t\tpassType: 'password',\r\n\t\t\t\terror: ''\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t\t// Getting Vuex State from store/modules/user\r\n\t\t\t...mapState({\r\n\t\t\t\temail: state => state.user.temp_email,\r\n\t\t\t\ttoken: state => state.user.temp_token\r\n\t\t\t})\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tshowPwd: function() {\r\n\t\t\t\tthis.isShow = this.isShow ? 0 : 1\r\n\t\t\t\tthis.isShow ? this.passType = 'text' : this.passType = 'password'\r\n\t\t\t},\r\n\t\t\tupdateData: function(e) {\r\n\t\t\t\t//v-model from handling\r\n\t\t\t\tthis.$store.commit({\r\n\t\t\t\t\ttype: 'updateData',\r\n\t\t\t\t\tname: e.target.name,\r\n\t\t\t\t\tvalue: e.target.value\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\tbeforeSubmit: function() {\r\n\t\t\t\tthis.isDisabled = true\r\n\t\t\t\tthis.loginState = '正在登陆...'\r\n\t\t\t\tconsole.log(\"beforeSubmit...\")\r\n\t\t\t},\r\n\t\t\tonSuccess: function() {\r\n\t\t\t\tthis.$router.push({\r\n\t\t\t\t\tname: 'StatusView'\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\tonError: function(err) {\r\n\t\t\t\tconsole.log(\"onError...\" + err)\r\n\t\t\t\tthis.error = err.response.body.error\r\n\t\t\t\tthis.isDisabled = false\r\n\t\t\t\tthis.loginState = '登陆'\r\n\t\t\t},\r\n\t\t\tonSubmit: function() {\r\n\t\t\t\tthis.beforeSubmit()\r\n\t\t\t\tthis.$store.dispatch({\r\n\t\t\t\t\ttype: 'login',\r\n\t\t\t\t\temail: this.email,\r\n\t\t\t\t\ttoken: this.token\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tthis.onSuccess(res)\r\n\t\t\t\t}, err => {\r\n\t\t\t\t\tthis.onError(err)\r\n\t\t\t\t})\r\n\t\t\t}\r\n\t\t},\r\n\t\tbeforeRouteEnter(to, from, next) {\r\n\t\t\tnext(vm => {\r\n\t\t\t\tif (vm.$store.getters.currentUser.email) {\r\n\t\t\t\t\tvm.$router.push({\r\n\t\t\t\t\t\tname: 'StatusView'\r\n\t\t\t\t\t})\r\n\t\t\t\t} else {\r\n\t\t\t\t\tnext()\r\n\t\t\t\t}\r\n\t\t\t})\r\n\t\t},\n\t\tcreated(){\n\t\t\t// Getting local user automatic filling\n\t\t\tif(localStorage.getItem('email')){\n\t\t\t\tthis.$store.commit({\n\t\t\t\t\ttype:'getLocalUser'\n\t\t\t\t})\n\t\t\t}\n\t\t}\n\t}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\t.login-view {\r\n\t\th1 {\r\n\t\t\theight: 4.5rem;\r\n\t\t\tmargin: 0 0 1rem 0;\r\n\t\t\tpadding: 0 1.8rem;\r\n\t\t\tline-height: 4.5rem;\r\n\t\t\tbackground: #fff;\r\n\t\t\tborder-bottom: 0.1rem solid #eee;\r\n\t\t\ttext-align: center;\r\n\t\t\tfont-size: 1.8rem;\r\n\t\t\tfont-weight: bold;\r\n\r\n\t\t\ta {\r\n\t\t\t\tposition: absolute;\r\n\t\t\t\tleft: 1.8rem;\r\n\t\t\t\ttop: 0;\r\n\t\t\t\tcolor: #42bd56;\r\n\t\t\t\tfont-size: 1.5rem;\r\n\t\t\t\tfont-weight: normal;\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tform {\r\n\t\t\tpadding: 2rem 1.5rem;\r\n\r\n\t\t\tstrong {\r\n\t\t\t\tfont-size: 1.5rem;\r\n\t\t\t\tcolor: #222;\r\n\t\t\t\tdisplay: none;\r\n\t\t\t\tmargin-bottom: 0.5rem;\r\n\t\t\t}\r\n\r\n\t\t\tinput[type=\"email\"],\r\n\t\t\tinput[type=\"text\"],\r\n\t\t\tinput[type=\"password\"] {\r\n\t\t\t\tdisplay: inline-block;\r\n\t\t\t\twidth: 100%;\r\n\t\t\t\theight: 4.4rem;\r\n\t\t\t\tpadding: 0 0.8rem;\r\n\t\t\t\tbox-sizing: border-box;\r\n\t\t\t\tfont-size: 1.5rem;\r\n\t\t\t\tbackground: #fff;\r\n\t\t\t\tborder: 0.1rem solid #ccc;\r\n\t\t\t\tborder-top-left-radius: 0.3rem;\r\n\t\t\t\tborder-top-right-radius: 0.3rem;\r\n\t\t\t\toutline: 0;\r\n\t\t\t}\r\n\r\n\t\t\t.from-pwd {\r\n\t\t\t\tposition: relative;\r\n\r\n\t\t\t\tinput {\r\n\t\t\t\t\tpadding-right: 4rem;\r\n\t\t\t\t\tborder-top: 0;\r\n\t\t\t\t}\r\n\r\n\t\t\t\t.show-pwd {\r\n\t\t\t\t\tposition: absolute;\r\n\t\t\t\t\tright: 0.2rem;\r\n\t\t\t\t\ttop: 0;\r\n\t\t\t\t\tdisplay: block;\r\n\t\t\t\t\theight: 100%;\r\n\t\t\t\t\twidth: 3.2rem;\r\n\t\t\t\t\tbackground-image: url(data:image/png;base64,);\r\n\t\t\t\t\tbackground-repeat: no-repeat;\r\n\t\t\t\t\tbackground-position: center center;\r\n\t\t\t\t\tbackground-size: 2.2rem;\r\n\t\t\t\t\tz-index: 3;\r\n\t\t\t\t}\r\n\r\n\t\t\t\t.show-pwd.show {\r\n\t\t\t\t\tbackground-image: url(data:image/png;base64,);\r\n\t\t\t\t}\r\n\t\t\t}\r\n\r\n\t\t\t.submit {\r\n\t\t\t\tcursor: pointer;\r\n\t\t\t\twidth: 100%;\r\n\t\t\t\tpadding: 1.2rem 2.6rem;\r\n\t\t\t\tmargin-top: 1rem;\r\n\t\t\t\tfont-size: 1.7rem;\r\n\t\t\t\ttext-align: center;\r\n\t\t\t\tcolor: #fff;\r\n\t\t\t\tbackground: #17AA52;\r\n\t\t\t\tborder: 0.1rem solid #17AA52;\r\n\t\t\t\tborder-radius: 0.3rem;\r\n\t\t\t}\r\n\r\n\t\t\t.disabled {\r\n\t\t\t\tcursor: not-allowed;\r\n\t\t\t\tbackground: #eee;\r\n\t\t\t\tborder: none;\r\n\t\t\t}\r\n\r\n\t\t\t.tip {\r\n\t\t\t\tfont-size: 1.4rem;\r\n\t\t\t\tcolor: #aaa;\r\n\t\t\t}\r\n\r\n\t\t\t.error {\r\n\t\t\t\tcolor: #ff0000;\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\t.footer {\r\n\t\t\t.more-login {\r\n\t\t\t\tfont-size: 1.5rem;\r\n\t\t\t\tcolor: #aaa;\r\n\t\t\t\ttext-align: center;\r\n\t\t\t}\r\n\r\n\t\t\t.btns {\r\n\t\t\t\tmargin-top: 4rem;\r\n\t\t\t\ttext-align: center;\r\n\t\t\t\tfont-size: 1.5rem;\r\n\r\n\t\t\t\ta {\r\n\t\t\t\t\tcolor: #42bd56;\r\n\t\t\t\t\tmargin-right: 1.5rem;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</style>\n"]}]}